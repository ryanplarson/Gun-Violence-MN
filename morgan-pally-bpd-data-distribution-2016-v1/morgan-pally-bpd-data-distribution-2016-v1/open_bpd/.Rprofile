.First <- function () {
  #require("utils")
  if (!("pacman" %in% rownames(installed.packages()))) {
    install.packages("pacman")
  }

  pacman::p_load("knitr", "magrittr", "xts","zoo","plyr", "stats", "ggplot2",
                 "tscount", "sp", "foreign","scales",
                 "maptools", "gpclib", "proto", "labeling", "multinomRob", "dplyr", "car", "gmodels",
                 "scales", "tseries", "forecast", "stargazer", "readr")
}
